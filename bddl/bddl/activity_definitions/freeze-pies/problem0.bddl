(define (problem freeze-pies_0)
    (:domain omnigibson)

    (:objects
        pie.n.01_1 pie.n.01_2 - pie.n.01
        plate.n.04_1 plate.n.04_2 - plate.n.04
        oven.n.01_1 - oven.n.01
        tupperware.n.01_1 tupperware.n.01_2 - tupperware.n.01
        cabinet.n.01_1 - cabinet.n.01
        water.n.06_1 - water.n.06
        sink.n.01_1 - sink.n.01
        floor.n.01_1 - floor.n.01
        electric_refrigerator.n.01_1 - electric_refrigerator.n.01
        agent.n.01_1 - agent.n.01
    )
    
    (:init 
        (ontop pie.n.01_1 plate.n.04_1) 
        (ontop pie.n.01_2 plate.n.04_2) 
        (ontop plate.n.04_1 oven.n.01_1) 
        (ontop plate.n.04_2 oven.n.01_1) 
        (inside tupperware.n.01_1 cabinet.n.01_1) 
        (inside tupperware.n.01_2 cabinet.n.01_1) 
        (not 
            (frozen pie.n.01_1)
        ) 
        (not 
            (frozen pie.n.01_2)
        ) 
        (insource sink.n.01_1 water.n.06_1) 
        (inroom floor.n.01_1 kitchen) 
        (inroom cabinet.n.01_1 kitchen) 
        (inroom sink.n.01_1 kitchen) 
        (inroom oven.n.01_1 kitchen) 
        (inroom electric_refrigerator.n.01_1 kitchen) 
        (ontop agent.n.01_1 floor.n.01_1)
    )
    
    (:goal 
        (and 
            (forpairs 
                (?pie.n.01 - pie.n.01) 
                (?tupperware.n.01 - tupperware.n.01) 
                (inside ?pie.n.01 ?tupperware.n.01)
            ) 
            (forall 
                (?tupperware.n.01 - tupperware.n.01) 
                (inside ?tupperware.n.01 ?electric_refrigerator.n.01_1)
            ) 
            (forall 
                (?pie.n.01 - pie.n.01) 
                (frozen ?pie.n.01)
            )
        )
    )
)
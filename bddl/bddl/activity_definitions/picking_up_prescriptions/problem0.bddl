(define (problem picking_up_prescriptions-0)
    (:domain omnigibson)

    (:objects
        floor.n.01_1 - floor.n.01
        pill_bottle.n.01_1 pill_bottle.n.01_2 - pill_bottle.n.01
        bottle__of__antihistamines.n.01_1 bottle__of__antihistamines.n.01_2 - bottle__of__antihistamines.n.01
        shelf.n.01_1 shelf.n.01_2 - shelf.n.01
        plastic_bag.n.01_1 - plastic_bag.n.01
        checkout_counter.n.01_1 - checkout_counter.n.01
        printer.n.03_1 - printer.n.03
        desktop_computer.n.01_1 - desktop_computer.n.01
        sheet.n.02_1 - sheet.n.02
        agent.n.01_1 - agent.n.01
    )

    (:init
        (inside pill_bottle.n.01_1 shelf.n.01_1)
        (inside pill_bottle.n.01_2 shelf.n.01_1)
        (inside bottle__of__antihistamines.n.01_1 shelf.n.01_2)
        (inside bottle__of__antihistamines.n.01_2 shelf.n.01_2)
        (ontop plastic_bag.n.01_1 checkout_counter.n.01_1)
        (ontop printer.n.03_1 checkout_counter.n.01_1)
        (ontop desktop_computer.n.01_1 checkout_counter.n.01_1)
        (ontop sheet.n.02_1 printer.n.03_1)
        (inroom floor.n.01_1 grocery_store)
        (inroom shelf.n.01_1 grocery_store)
        (inroom shelf.n.01_2 grocery_store)
        (inroom desk.n.01_1 grocery_store)
        (ontop agent.n.01_1 floor.n.01_1)
    )

    (:goal
        (and
            (forall
                (?pill_bottle.n.01 - pill_bottle.n.01)
                (inside ?pill_bottle.n.01 ?plastic_bag.n.01_1)
            )
            (forall
                (?bottle__of__antihistamines.n.01 - bottle__of__antihistamines.n.01)
                (inside ?bottle__of__antihistamines.n.01 ?plastic_bag.n.01_1)
            )
        )
    )
)
